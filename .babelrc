{
  "plugins": [
    ["module:react-native-dotenv", {
      "moduleName": "@env",
      "path": ".env",
      "blacklist": null,
      "whitelist": null,
      "safe": false,
      "allowUndefined": false
    }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "~components": "./src/components",
        "~screens": "./src/screens"
      }
    }]
  ]
}
